//----------------------------------------------------------
// launch属性の場合、デバッグ開始時に実行するファイルを指定するためfile属性を使用する
//----------------------------------------------------------
{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "My debug",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            // "program": "${workspaceFolder}",
            "console": "integratedTerminal",
			"sudo": true,
        }
    ]
}